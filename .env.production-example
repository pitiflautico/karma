
# Production Session Configuration for WebView + OAuth
APP_URL=https://feelith.com
SESSION_DRIVER=database
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=none
SESSION_DOMAIN=.feelith.com
SESSION_PARTITIONED_COOKIE=false

# Important: SameSite=none requires Secure=true
# This allows session cookies to work in WebView during OAuth redirects
